  "description": "A save editor for Mass Effect Trilogy (and Legendary)",
  "dependencies": {
    "autoprefixer": "^10.3.4",
    "postcss": "^8.3.6",
    "tailwindcss": "^2.2.11"
  },
  "scripts": {
